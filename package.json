    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.5.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "^0.67.12",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "msw": "^0.39.2",
    "react-test-renderer": "17.0.1",
    "typescript": "~4.3.5"
