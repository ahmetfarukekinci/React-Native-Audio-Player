    "formik": "^2.2.9",
    "metro-react-native-babel-preset": "^0.69.0",
    "react": "17.0.1",
    "react-devtools": "^4.23.0",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-reanimated": "~2.3.1",
