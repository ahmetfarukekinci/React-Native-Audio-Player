{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg/.*|react-native-svg-transformer/.*)"
    ],
    "setupFilesAfterEnv": [
      "./jest/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/slider": "4.2.1",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/react-redux": "^7.1.24",
    "babel-plugin-module-resolver": "^4.1.0",
    "expo": "^45.0.6",
    "expo-app-loading": "~2.0.0",
    "expo-asset": "~8.5.0",
    "expo-av": "~11.2.3",
    "expo-constants": "~13.1.1",
    "expo-font": "~10.1.0",
    "expo-linear-gradient": "~11.3.0",
    "expo-linking": "~3.1.0",
    "expo-secure-store": "~11.2.0",
    "expo-splash-screen": "^0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-web-browser": "~10.2.1",
    "formik": "^2.2.9",
    "metro-react-native-babel-preset": "^0.69.1",
    "react": "17.0.2",
    "react-devtools": "^4.25.0",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-responsive-fontsize": "^0.5.1",
